<%- include('../partials/header')%>


<%- include('../partials/aside')%>
<main role="main" class="probootstrap-main js-probootstrap-main">
    <div class="probootstrap-bar">
      <a href="#" class="probootstrap-toggle js-probootstrap-toggle"><span class="oi oi-menu"></span></a>
      <div class="probootstrap-main-site-logo"><a href="/">HALAL</a></a></div>
    </div>
    <div class="container-fluid">
      <div class="row justify-content-center">
        <div class="col-xl-8 col-lg-12">
        
         <% contacts.forEach(function(contact){%>
            <h5><strong>Name:</strong> <%= contact.fname%> <%= contact.lname%> |  <strong>Email:</strong>  <%= contact.email%></h5> 
            <p><%= contact.message%></p>
            <form action="/admin/contact/<%= contact._id%>?_method=DELETE" method="POST">
              <button class="btn btn-dark">DELETE</button>
            </form>
         <%})%>

          
        </div>
      </section>
      <!-- END section -->

    </div>

    <div class="container-fluid d-md-none">
      <div class="row">
        <div class="col-md-12">
          <ul class="list-unstyled d-flex probootstrap-aside-social">
            <li><a href="#" class="p-2"><span class="icon-twitter"></span></a></li>
            <li><a href="#" class="p-2"><span class="icon-instagram"></span></a></li>
            <li><a href="#" class="p-2"><span class="icon-dribbble"></span></a></li>
          </ul>
          <p>&copy; 2017 <a href="https://probootstrap.com/" target="_blank">ProBootstrap:Aside</a>. <br> All Rights Reserved. Designed by <a href="https://probootstrap.com/" target="_blank">ProBootstrap.com</a></p>
        </div>
      </div>
    </div>

  </main>
  <%- include('../partials/footer')%>
